# n8n Secrets
# Copy to secret.yaml and fill in real values
# Apply with: kubectl apply -f secret.yaml
apiVersion: v1
kind: Secret
metadata:
  name: n8n-secret
  namespace: hydra-infra
type: Opaque
stringData:
  postgres-user: "n8n"
  postgres-password: "CHANGE-ME"
  postgres-db: "n8n"
  n8n-encryption-key: "CHANGE-ME-generate-with-openssl-rand-hex-32"
  n8n-user-manager-api-key: "CHANGE-ME-generate-with-openssl-rand-hex-32"
